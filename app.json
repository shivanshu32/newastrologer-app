{
  "expo": {
    "name": "Jyotish Call - Astrologer",
    "slug": "jyotishcall-astrologer-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-square.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jyotishcall.astrologer"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-square.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jyotishcall.astrologer"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "df7c8590-c046-4d34-bacb-908b2207ea73"
      }
    },
    "owner": "shivanshu32"
  }
}
